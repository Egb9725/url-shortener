<%- include("./partials/header.ejs",{title:"Update URL" }) %>

<h1>Modifier l'URL</h1>
<form id="updateForm" data-short-url="<%= url.short %>">
    <label for="fullUrl">Nouveau URL:</label>
    <input type="text" id="fullUrl" name="fullUrl" value="<%= url.full %>" required>
    <button type="submit">Enregistrer</button>
</form>


